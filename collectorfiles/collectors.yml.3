default:
  rabbitmq:
    host: localhost
    port: 5672
    vhost: /
    exchange: stucco
    queue: stucco
    login: stucco
    password: stucco
    message_size_limit: 10485760
  stucco:
    document-service:
      host: localhost
      port: 8118

  demo:
    collectors:
      -
        type: RSS
        data-type: unstructured
        source-name: Oracle Alerts
        source-URI: http://www.oracle.com/ocom/groups/public/@otn/documents/webcontent/rss-otn-sec.xml
        post-process: removeHTML
        content-type : text/html
        now-collect: all
        cron: 0 3 * * * ?
    outputLog : ./demo.log
    outputDir : ./data/Receive
